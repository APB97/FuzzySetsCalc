@model FormFileModel

<form enctype="multipart/form-data" method="post">
    <dl>
        <dt>
            <label>File</label>
        </dt>
        <dd>
            <input asp-for="FormFile" type="file">
            <span asp-validation-for="FormFile"></span>
        </dd>
    </dl>
    <input asp-page-handler="Load" class="btn-primary" type="submit" value="Upload" />
</form>
